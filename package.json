{
    "name": "crud",
    "version": "1.0.0",
    "description": "UI5 Application: crud",
    "author": "kevin-gram",
    "license": "Apache-2.0",
    "scripts": {
        "clean": "rimraf dist coverage",
        "build": "ui5 build --clean-dest",
        "build:opt": "ui5 build self-contained --clean-dest --all",
        "start": "ui5 serve --port 8080 -o index.html",
        "start-cdn": "ui5 serve --port 8080 -o index-cdn.html",
        "start:dist": "npm start -- --config ui5-dist.yaml",
        "start:dist-cdn": "npm run start-cdn -- --config ui5-dist.yaml",
        "lint": "eslint webapp",
        "karma": "karma start",
        "karma-ci": "karma start karma-ci.conf.js",
        "karma-ci-cov": "karma start karma-ci-cov.conf.js",
        "test": "npm run lint && npm run karma-ci-cov"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.10",
        "@types/webdriverio": "^4.13.3",
        "@ui5/cli": "^4.0.6",
        "@ui5/middleware-code-coverage": "^2.0.0",
        "@wdio/cli": "^9.4.2",
        "@wdio/local-runner": "^9.4.2",
        "@wdio/mocha-framework": "^9.4.2",
        "@wdio/spec-reporter": "^9.4.2",
        "chromedriver": "^131.0.3",
        "eslint": "^9.9.1",
        "eslint-plugin-jsdoc": "^50.2.2",
        "globals": "^15.9.0",
        "karma": "^6.4.4",
        "karma-chrome-launcher": "^3.2.0",
        "karma-coverage": "^2.2.1",
        "karma-ui5": "^4.0.1",
        "rimraf": "^6.0.1",
        "ui5-middleware-livereload": "^3.0.3",
        "webdriverio": "^9.4.2"
    },
    "dependencies": {
        "16": "^0.0.2",
        "cors": "^2.8.5",
        "express": "^4.21.1",
        "http-proxy-middleware": "^3.0.3",
        "livereload": "^0.9.3"
    },
    "main": "eslint.config.js",
    "directories": {
        "test": "test"
    },
    "keywords": []
}
